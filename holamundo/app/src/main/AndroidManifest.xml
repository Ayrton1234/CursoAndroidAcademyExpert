<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tracking.brian.holamundo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".layoutEjemplos.constraintLayout.ConstraintLayout">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Main2Activity" />
        <activity
            android:name=".HolaActivity"
            android:label="@string/title_activity_constraint_layout" />
        <activity
            android:name=".layoutEjemplos.frameLayout.FrameLayout"
            android:label="@string/title_activity_frame_layout" />
        <activity
            android:name=".layoutEjemplos.LinearLayout.LinearLayout"
            android:label="@string/title_activity_linear_layout" />
        <activity
            android:name=".layoutEjemplos.relativeLayout.RelativeLayout"
            android:label="@string/title_activity_relative_layout"></activity>
    </application>

</manifest>